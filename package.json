{
    "homepage": "https://railmapgen.github.io/rmg-runtime",
    "name": "@railmapgen/rmg-runtime",
    "repository": "github:railmapgen/rmg-runtime",
    "bugs": {
        "url": "https://github.com/railmapgen/rmg-runtime/issues"
    },
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/node": "^16.11.47",
        "@typescript-eslint/eslint-plugin": "^5.33.1",
        "@typescript-eslint/parser": "^5.33.1",
        "concurrently": "^7.3.0",
        "eslint": "^8.22.0",
        "eslint-plugin-prettier": "^4.2.1",
        "http-server": "^14.1.1",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "scripts": {
        "start": "concurrently \"npm run dev:package\" \"npm run dev:webpack\" \"npm run dev:server\" -n \"package,webpack,server\" -c \"blue.bold,magenta.bold,yellow.bold\"",
        "dev:server": "http-server ./",
        "dev:package": "tsc -w -p ./tsconfig.prd.json",
        "dev:webpack": "webpack --watch",
        "build": "tsc -p ./tsconfig.prd.json",
        "test": "echo \"No tests found\"",
        "lint": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "0.1.4",
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    },
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist"
    ]
}
