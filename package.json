{
    "homepage": "https://railmapgen.github.io/rmg-runtime",
    "name": "rmg-runtime",
    "repository": "github:railmapgen/rmg-runtime",
    "bugs": {
        "url": "https://github.com/railmapgen/rmg-runtime/issues"
    },
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.47",
        "eslint-plugin-prettier": "^4.2.1",
        "http-server": "^14.1.1",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4"
    },
    "scripts": {
        "start": "concurrently \"npm run dev:package\" \"npm run dev:server\" -n \"package,server\" -c \"blue.bold,magenta.bold\"",
        "dev:server": "http-server ./",
        "dev:package": "tsc -w -p ./tsconfig.prd.json",
        "build": "tsc -p ./tsconfig.prd.json",
        "test": "echo \"No tests found\"",
        "lint": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "0.0.1",
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}
