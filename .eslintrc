{
    "extends": "eslint:recommended",
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ]
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "ignorePatterns": [
        "webpack.config.js"
    ]
}
